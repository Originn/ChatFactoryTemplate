{
  "name": "chatbot-template",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "ChatFactory",
  "scripts": {
    "dev": "next dev",
    "sync-env": "vercel link --yes --project=testbot && powershell -Command \"if (Test-Path .env.local) { Copy-Item .env.local .env.old -Force } elseif (Test-Path .env) { Copy-Item .env .env.old -Force }\" && vercel env pull --yes .env.local && powershell -ExecutionPolicy Bypass -File scripts/clean-env.ps1",
    "build": "npm run sync-env >nul 2>&1 || echo Sync failed, continuing... && next build",
    "build:prod": "next build",
    "build:vercel": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "export": "next export",
    "delete-old-chat-history": "node scripts/deleteOldChatHistory.js",
    "gdpr-anonymize-data": "node scripts/monthly-anonymization.js",
    "test-template": "node test-template-deployment.js",
    "install:force": "npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/storage": "^6.12.0",
    "@huggingface/inference": "^4.4.0",
    "@langchain/cohere": "^0.3.4",
    "@langchain/community": "^0.3.48",
    "@langchain/core": "^0.3.42",
    "@langchain/openai": "^0.4.5",
    "@langchain/pinecone": "^0.0.7",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@pinecone-database/pinecone": "^2.0.1",
    "@radix-ui/react-accordion": "^1.1.1",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase": "^11.6.1",
    "firebase-admin": "^12.1.1",
    "formidable": "^3.5.1",
    "js-cookie": "^3.0.5",
    "langchain": "0.3.7",
    "lucide-react": "^0.125.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "next": "^14.2.18",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.0",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.4.15",
    "wavesurfer.js": "^7.8.0",
    "zod": "3.25.32"
  },
  "resolutions": {
    "zod": "3.25.32",
    "ignore": "^7.0.5"
  },
  "overrides": {
    "zod": "3.25.32",
    "ignore": "^7.0.5"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "prettier": "^2.8.4",
    "typescript": "^5.6.0"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "keywords": [
    "chatbot",
    "ai",
    "gpt4",
    "pinecone",
    "typescript",
    "nextjs",
    "langchain",
    "pdf",
    "openai"
  ]
}
